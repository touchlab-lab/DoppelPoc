# Uncomment this line to define a global platform for your project
# platform :ios, '8.0'
# Uncomment this line if you're using Swift
# use_frameworks!


# J2ObjC Gradle Plugin - PodMethods - DO NOT MODIFY START - can be moved as a block
def j2objc_shared
    pod 'j2objc-shared-debug', :configuration => ['Debug'], :path => '../lib/build/j2objcOutputs'
    pod 'j2objc-shared-release', :configuration => ['Release'], :path => '../lib/build/j2objcOutputs'
    pod 'Gson', :path => '../../Gson'
    pod 'Dagger2', :path => '../../Dagger2'
    pod 'Okhttp', :path => '../../Okhttp'
    pod 'RxJava', :path => '../../RxJava'
    pod 'Retrofit', :path => '../../Retrofit'
    pod 'AndroidBase', :path => '../../AndroidBase'
    pod 'SqueakyCore', :path => '../../SqueakyCore'
    pod 'J2OBJC', :path => '../../J2OBJC'
end
# J2ObjC Gradle Plugin - PodMethods - DO NOT MODIFY END

target 'ios' do
    platform :ios, '8.3'
    j2objc_shared

end

target 'iosTests' do
    platform :ios, '8.3'
    j2objc_shared

end

target 'iosUITests' do

end
